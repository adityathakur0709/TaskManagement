@model IEnumerable<Management.Models.User>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}

<h1>Welcome Admin</h1>


<table class="table">
    <thead>
        <tr>
            <th>User Name</th>
            <th>Password</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var task in Model)
        {
            <tr>
                <td>@task.userName</td>
                <td>@task.password</td>
            </tr>
        }
    </tbody>
</table>
<a asp-action="LogOut" class="btn btn-primary float-end">LogOut</a>
<a asp-action="Index" class="btn btn-primary float-start">Back to Index</a>

<style>
  
    .table {
        width: 100%;
        border-collapse: collapse;
        margin-top: 20px;
    }

        .table th, .table td {
            padding: 8px;
            text-align: left;
            border-bottom: 1px solid #ddd;
        }

        .table th {
            background-color: #f2f2f2;
        }
</style>

